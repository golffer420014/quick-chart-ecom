generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id        String   @id @map("_id") // เปลี่ยนจาก @db.ObjectId เป็น String ปกติ
  name      String
  email     String   @unique
  imageUrl  String
  cartItems Json     @default("{}")
}
