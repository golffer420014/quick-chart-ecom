generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String   @unique
  imageUrl  String
  cartItems Json     @default("{}") // ใช้ Json type และกำหนดค่าเริ่มต้นเป็น {}
}
